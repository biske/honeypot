<h3>Home</h3>
<% @users.each do |user| %>
  <p>User: <%=link_to user.name, user %></p>
<% end %>

<% if user_signed_in? %>
	<p><%= link_to("Sign out", destroy_user_session_path, :method => :delete) %></p>
	<p><%= link_to("Edit account", edit_user_registration_path(current_user)) %></p>
<% else %>
	<p><%= link_to("Sign in", new_user_session_path) %></p>
<% end %>
